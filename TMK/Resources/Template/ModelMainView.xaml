<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TMK.Screen.ViewModels"
                    xmlns:views="clr-namespace:TMK.Screen.Views"
                    >


    <DataTemplate DataType="{x:Type vm:TubesViewModel}">
        <views:PipesView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:BundleViewModel}">
        <views:BundlesView DataContext="{Binding}"/>
    </DataTemplate>
    
</ResourceDictionary>